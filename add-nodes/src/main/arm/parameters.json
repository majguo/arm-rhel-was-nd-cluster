{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/${git.proj}/${git.tag}/${project.artifactId}/src/main/"
        },
        "numberOfNodes": {
            "value": "${numberOfNodes}"
        },
        "managedVMPrefix": {
            "value": "${managedVMPrefix}"
        },
        "dnsLabelPrefix":{
            "value": "${dnsLabelPrefix}"
        },
        "vmAdminId": {
            "value": "${vmAdminId}"
        },
        "vmAdminPwd": {
            "value": "${vmAdminPwd}"
        },
        "adminUser": {
            "value": "${adminUser}"
        },
        "adminPwd": {
            "value": "${adminPwd}"
        },
        "clusterName": {
            "value": "${clusterName}"
        },
		"nodeGroupName": {
			"value": "${nodeGroupName}"
        },
        "coreGroupName": {
			"value": "${coreGroupName}"
        },
        "dmgrHostName": {
			"value": "${dmgrHostName}"
        },
        "dmgrPort": {
			"value": "${dmgrPort}"
        },
        "vNetName": {
            "value": "${vNetName}"
        },
        "subnetName": {
            "value": "${subnetName}"
		}
    }
}