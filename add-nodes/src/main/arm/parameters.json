{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ibmUserId": {
            "value": "${ibmUserId}"
        },
        "ibmUserPwd": {
            "value": "${ibmUserPwd}"
        },
        "numberOfNodes": {
            "value": "${numberOfNodes}"
        },
        "managedVMPrefix": {
            "value": "${managedVMPrefix}"
        },
        "dnsLabelPrefix":{
            "value": "${dnsLabelPrefix}"
        },
        "vmAdminId": {
            "value": "${vmAdminId}"
        },
        "vmAdminPwd": {
            "value": "${vmAdminPwd}"
        },
        "vNetName": {
            "value": "${vNetName}"
        },
        "subnetName": {
            "value": "${subnetName}"
		}
    }
}